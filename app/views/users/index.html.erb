<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="span6 offset3">
<%= search_form_for @search do |f| %>
  <div class="field">
    <%= f.label :name_cont, "Name contains" %>
    <%= f.text_field :name_cont %>
  </div>
  <div class="actions"><%= f.submit "Search", class: "btn btn-large btn-primary" %></div>
<% end %>
 </div>
</div>


<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>